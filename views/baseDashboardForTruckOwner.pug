doctype html
html(lang='en')
  head
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    link(rel="icon" , href="/static/img/realLogo.jpg" type="image/jpg" sizes="12x12")     
    block style
        link(rel='stylesheet', href='../static/style/dashboardStyle.css')
        link(rel='stylesheet', href='../static/style/viewBiddingsStyle.css')

        link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css')
        link(href='https://fonts.googleapis.com/css2?family=Fredoka+One&display=swap', rel='stylesheet')
        link(href='https://fonts.googleapis.com/css2?family=Mr+Dafoe&display=swap', rel='stylesheet')
  body
    div#wrapper1.complete-wrapper-of-page  
    div#wrapper2.complete-wrapper-of-page  
    nav#navbar
      #navbar-overlay
      #inside-navbar
        a#logo(href="/").logo-image
          img.logo-image(src="../static/img/realLogo.jpg", alt="", srcset="")

      .bars(onclick='navbarBelow720px(this)')
          .bar1
          .bar2
          .bar3

    #sidebar-main-section-wrapper
        block content    
    // HOVER ON OUR COMPANY IN BUTTON
    section#hover-on-menu
      .div-inside-section-of-navbar-links
        #arrow
          .arrow-up
        .Allitems-ofdiv-inside-section-of-navbar-links
          ul#list-of-hoverMenu
            a.item-hoverMenu(href='')
              li Help Center
            a.item-hoverMenu(href='/logout')
              li#lastItemOfHoverMenu Contact Us
    block script          
        script(src='https://code.jquery.com/jquery-3.5.1.min.js', integrity='sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=', crossorigin='anonymous')    
        script(src='../static/script/dashboardScript.js')
        script(src='../static/script/homeScript.js')
